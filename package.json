{
  "name": "contract-management-system",
  "version": "1.0.0",
  "description": "Professional enterprise-grade contract management and generation system",
  "private": true,
  "author": "Contract Management System Team",
  "license": "MIT",
  "homepage": "https://github.com/yourusername/contract-management-system#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/contract-management-system.git"
  },
  "keywords": [
    "contract-management",
    "nextjs",
    "typescript",
    "supabase",
    "rbac",
    "enterprise",
    "saas"
  ],
  "scripts": {
    "rbac:seed": "supabase db query < scripts/seed_rbac.sql",
    "rbac:test-setup": "node scripts/test-rbac-setup.js",
    "rbac:test-simple": "node scripts/test-rbac-simple.js",
    "rbac:assign-test": "node scripts/assign-test-role.js",
    "rbac:docs": "echo 'RBAC docs updated'",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:ci": "npm run lint && npm run type-check && npm run test && npm run build",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "db:seed": "supabase db seed",
    "db:studio": "supabase studio",
    "functions:deploy": "supabase functions deploy",
    "vercel-build": "npm run build",
    "vercel-dev": "npm run dev",
    "rbac:migrate": "supabase db push",
    "rbac:test": "jest --testPathPattern=rbac",
    "rbac:drift": "tsx scripts/rbac_drift_check.ts",
    "rbac:lint": "tsx scripts/rbac_guard_lint.ts",
    "rbac:smoke": "tsx scripts/rbac_smoke_test.ts",
    "rbac:fix": "node scripts/apply_rbac_fixes.js",
    "rbac:fix:simple": "node scripts/apply_rbac_fixes_simple.js",
    "rbac:help": "node scripts/get_supabase_keys.js",
    "rbac:check": "node scripts/check_current_permissions.js",
    "rbac:flip:enforce": "echo 'Set RBAC_ENFORCEMENT=enforce in env and redeploy'",
    "rbac:flip:dryrun": "echo 'Set RBAC_ENFORCEMENT=dry-run in env and redeploy'",
    "fix:luxsess2001": "node scripts/fix_luxsess2001_role.js",
    "sync:roles": "node scripts/sync_all_user_roles.js",
    "sync:roles:simple": "node scripts/sync_all_user_roles_simple.js",
    "sync:roles:fixed": "node scripts/sync_all_user_roles_fixed.js",
    "sync:roles:final": "node scripts/sync_all_user_roles_final.js",
    "sync:roles:perfect": "node scripts/sync_all_user_roles_constraint_fixed.js",
    "sync:roles:100": "node scripts/sync_all_user_roles_final_perfect.js",
    "sync:roles:upsert": "node scripts/sync_all_user_roles_upsert.js",
    "debug:cleaning": "node scripts/debug_cleaning_provider.js",
    "fix:corrupted": "node scripts/fix_corrupted_user.js",
    "fix:corrupted:update": "node scripts/fix_corrupted_user_update.js",
    "sync:roles:skip": "node scripts/sync_all_user_roles_skip_problematic.js",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "security:update": "npm update",
    "clean": "rm -rf .next out dist build",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "reinstall": "npm run clean:all && npm install",
    "analyze": "ANALYZE=true npm run build",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "docs:dev": "next dev -p 3001",
    "docs:build": "next build",
    "docs:start": "next start -p 3001",
    "security:setup": "echo 'Setting up security features...' && npm run security:migrate && npm run security:test",
    "security:migrate": "supabase db push --include-all",
    "security:test": "jest --testPathPattern=security",
    "security:deploy-functions": "supabase functions deploy update-user-claims",
    "security:check-headers": "curl -I -s https://yourdomain.com | grep -E '(X-Frame-Options|X-Content-Type-Options|X-XSS-Protection)'",
    "security:test-rate-limit": "node scripts/test-rate-limiting.js",
    "security:test-mfa": "node scripts/test-mfa-setup.js",
    "security:test-cors": "node scripts/test-cors-config.js",
    "security:audit-full": "npm run security:audit && npm run security:test && npm run security:check-headers",
    "mfa:setup": "echo 'Setting up MFA system...' && npm run security:migrate",
    "mfa:test": "npm run security:test-mfa",
    "rate-limit:setup": "echo 'Setting up rate limiting...' && npm run security:migrate",
    "rate-limit:test": "npm run security:test-rate-limit",
    "cors:setup": "echo 'Setting up CORS...' && npm run security:migrate",
    "cors:test": "npm run security:test-cors",
    "hr:setup": "node scripts/setup-hr-module.js",
    "hr:migrate": "supabase db push --include-all",
    "hr:seed": "node scripts/setup-hr-module.js",
    "hr:test": "jest --testPathPattern=hr",
    "auth:fix-captcha": "node scripts/disable-captcha.js",
    "prod:check": "node scripts/check-production-config.js",
    "prod:deploy": "npm run prod:check && vercel --prod"
  },
