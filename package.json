    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern='(tests|__tests__)/.*\\.test\\.(ts|tsx)$' --coverage --coverageDirectory=coverage/unit",
    "test:integration": "jest --testPathPattern='(tests|__tests__)/.*\\.integration\\.test\\.(ts|tsx)$' --coverage --coverageDirectory=coverage/integration",
