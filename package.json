    "deploy:preview": "npm run deploy:validate && vercel --preview",
    "deploy:production": "npm run deploy:validate && vercel --prod",
    "ci:full": "npm run lint && npm run test:all && npm run db:health-check && npm run supabase:deploy",
    "ci:pre-deploy": "npm run db:health-check && npm run supabase:validate-migrations",
